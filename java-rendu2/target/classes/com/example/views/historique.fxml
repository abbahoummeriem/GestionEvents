                  <ImageView fitHeight="20.0" fitWidth="20.0">
                     <image>
                        <Image url="/com/example/images/reservation.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>  
            <Button mnemonicParsing="false" style="-fx-background-color: #133E87; -fx-font-weight: bold; -fx-border-color: #133E87; -fx-text-fill: WHITE;" text="historique" onAction="#historique" translateX="35.0" translateY="100.0"  >
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0">
                     <image>
                        <Image  url="/com/example/images/historique.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>          
            <Line endX="22.0" layoutX="22.0" layoutY="22.0" startX="-100.0" stroke="WHITE" translateY="77.0">
