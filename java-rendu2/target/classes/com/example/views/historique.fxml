        <VBox prefHeight="492.0" prefWidth="179.0" style="-fx-background-color: #133E87;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ImageView fx:id="imageView" fitHeight="88.0" fitWidth="98.0" pickOnBounds="true" preserveRatio="true" translateX="50.0" translateY="32.0">
