    <TableView fx:id="tableReservations" layoutX="180.0" layoutY="100.0" prefHeight="280.0" prefWidth="470.0">
        <columns>
           <TableColumn fx:id="colNomEvent" text="Nom de l'Événement" />
<TableColumn fx:id="colNomSalle" text="Nom de la Salle" />
<TableColumn fx:id="colNomTerrain" text="Nom du Terrain" />
<TableColumn fx:id="colDateEvent" text="Date de l'Événement" />


        </columns>
    </TableView>
