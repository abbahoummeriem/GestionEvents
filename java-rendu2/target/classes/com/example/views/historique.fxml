                  <Insets bottom="2.0" left="33.0" right="2.0" top="33.0" />
               </VBox.margin>
            </Line>
            <Button mnemonicParsing="false" style="-fx-background-color: #133E87; -fx-font-weight: bold; -fx-border-color: #133E87; -fx-text-fill: WHITE;" text="Logout" onAction="#onlogout" translateX="50.0" translateY="170.0"  >
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0">
                     <image>
                        <Image url="/com/example/images/out.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
