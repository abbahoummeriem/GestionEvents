            </Line>
            <Button mnemonicParsing="false" prefHeight="33.0" prefWidth="179.0" style="-fx-font-weight: bold; -fx-background-color: #133E87; -fx-text-fill: WHITE;"  text="Home"  onAction="#onHome" translateX="-15.0" translateY="74.0">
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="20.0">
                     <image>
                        <Image url="/com/example/images/home.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button mnemonicParsing="false" prefHeight="33.0" prefWidth="179.0" style="-fx-font-weight: bold; -fx-background-color: #133E87; -fx-text-fill: WHITE;" text="Réservation" onAction="#onReservation"  translateY="85.0" translateX="0.0" >
